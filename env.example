# LLM Navigator Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================

# Your Supabase project URL (required)
VITE_SUPABASE_URL=https://your-project-ref.supabase.co

# Your Supabase anonymous key (required)
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key_here

# Supabase service role key (for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_service_role_key_here

# =============================================================================
# STRIPE CONFIGURATION
# =============================================================================

# Stripe publishable key (required for frontend)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Stripe secret key (for server-side operations)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Stripe webhook secret (required for webhook verification)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# =============================================================================
# STRIPE PRICE IDs
# =============================================================================

# Starter plan price ID (required)
VITE_STRIPE_STARTER_PRICE_ID=price_your_starter_price_id_here

# Professional plan price ID (required)
VITE_STRIPE_PROFESSIONAL_PRICE_ID=price_your_professional_price_id_here

# Enterprise plan price ID (required)
VITE_STRIPE_ENTERPRISE_PRICE_ID=price_your_enterprise_price_id_here

# =============================================================================
# OPTIONAL CONFIGURATION
# =============================================================================

# Environment mode (development/production)
NODE_ENV=development

# App version
VITE_APP_VERSION=1.0.0

# Debug mode (set to true for additional logging)
VITE_DEBUG_MODE=false

# =============================================================================
# DEPLOYMENT NOTES
# =============================================================================

# For local development:
# 1. Copy this file to .env
# 2. Fill in your actual values
# 3. Never commit .env to version control

# For production deployment:
# 1. Set these variables in your hosting platform
# 2. Use production Stripe keys (pk_live_* and sk_live_*)
# 3. Use production Supabase project

